<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="Sonata\UserBundle\Command\TwoStepVerificationCommand" class="Sonata\UserBundle\Command\TwoStepVerificationCommand" public="true">
            <argument type="constant">null</argument>
            <argument type="service" id="sonata.user.google.authenticator.provider" on-invalid="null"/>
            <argument type="service" id="sonata.user.user_manager" on-invalid="null"/>
            <tag name="console.command"/>
        </service>
        <service id="sonata.user.command.activate_user" class="Sonata\UserBundle\Command\ActivateUserCommand">
            <argument type="service" id="sonata.user.util.user_manipulator"/>
            <tag name="console.command" command="sonata:user:activate"/>
        </service>
        <service id="sonata.user.command.change_password" class="Sonata\UserBundle\Command\ChangePasswordCommand">
            <argument type="service" id="sonata.user.util.user_manipulator"/>
            <tag name="console.command" command="sonata:user:change-password"/>
        </service>
        <service id="sonata.user.command.create_user" class="Sonata\UserBundle\Command\CreateUserCommand">
            <argument type="service" id="sonata.user.util.user_manipulator"/>
            <tag name="console.command" command="sonata:user:create"/>
        </service>
        <service id="sonata.user.command.deactivate_user" class="Sonata\UserBundle\Command\DeactivateUserCommand">
            <argument type="service" id="sonata.user.util.user_manipulator"/>
            <tag name="console.command" command="sonata:user:deactivate"/>
        </service>
        <service id="sonata.user.command.demote_user" class="Sonata\UserBundle\Command\DemoteUserCommand">
            <argument type="service" id="sonata.user.util.user_manipulator"/>
            <tag name="console.command" command="sonata:user:demote"/>
        </service>
        <service id="sonata.user.command.promote_user" class="Sonata\UserBundle\Command\PromoteUserCommand">
            <argument type="service" id="sonata.user.util.user_manipulator"/>
            <tag name="console.command" command="sonata:user:promote"/>
        </service>
    </services>
</container>
